{
  "version": 2,
  "name": "gitvis",
  "public": true,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "github": {
    "enabled": false
  },
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": ".*", "dest": "/index.html" }
  ],
  "build": {
    "env": {
      "VUE_APP_GRAPHQL_HTTP": "https://api.github.com/graphql"
    }
  }
}
